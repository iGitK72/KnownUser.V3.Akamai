{
  "name": "@queue-it/akamai-connector",
  "repository": "https://github.com/queueit/KnownUser.V3.Akamai",
  "license": "MIT",
  "main": "main.js",
  "version": "3.0.1",
  "type": "module",
  "scripts": {
    "postinstall": "postinstall",
    "prepare": "postinstall && gulp prepare",
    "build": "tsc && gulp packageForDeployment",

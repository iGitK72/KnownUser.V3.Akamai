    "serve": "gulp prepare && node --loader esm-fake-loader test/testServer.mjs"
  },
  "dependencies": {
    "queueit-knownuser": "^3.7.4"
  },
  "devDependencies": {
    "@types/node": "^14.17.32",
    "browserify": "^17.0.0",
    "esm-fake-loader": "^1.3.12",
    "fast-sha256": "^1.3.0",
    "gulp": "^4.0.2",
    "gulp-gzip": "^1.4.2",
    "gulp-modify-file": "^1.0.1",
    "gulp-tar": "^3.1.0",
    "node-fetch": "^3.0.0",
    "postinstall": "^0.7.4",
    "postinstall-browserify": "^0.7.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "prepare": {
    "queueit-knownuser/dist/index.js": {
      "command": "browserify",
      "output": "sdk/queueit-knownuserv3-sdk.js"
    }
  }
}
